{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/.github/workflows/ci.yml"
    ]
  },
  "nxCloudId": "697bfda413af7e7f03250f88",
  "plugins": [
    {
      "plugin": "@naxodev/gonx",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "testTargetName": "test",
        "lintTargetName": "lint",
        "tidyTargetName": "tidy"
      }
    }
  ],
  "targetDefaults": {
    "up": {
      "cache": false
    },
    "down": {
      "cache": false
    }
  }
}
