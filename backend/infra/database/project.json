{
  "name": "db",
  "projectType": "library",
  "sourceRoot": "backend/infra/database",
  "targets": {
    "up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "podman compose up -d",
        "cwd": "backend/infra/database"
      }
    },
    "down": {
      "executor": "nx:run-commands",
      "options": {
        "command": "podman compose down",
        "cwd": "backend/infra/database"
      }
    },
    "logs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "podman compose logs -f",
        "cwd": "backend/infra/database"
      }
    }
  }
}
